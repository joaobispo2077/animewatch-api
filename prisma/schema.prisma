// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Categories {
  id               String  @id @default(uuid())
  title            String
  color            String
  link             String?
  description      String?
  description_link String?
}
